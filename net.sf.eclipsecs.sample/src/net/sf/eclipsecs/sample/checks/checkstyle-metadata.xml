<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE checkstyle-metadata PUBLIC
"-//eclipse-cs//DTD Check Metadata 1.1//EN"
"http://eclipse-cs.sourceforge.net/dtds/checkstyle-metadata_1_1.dtd">
<checkstyle-metadata>
    <rule-group-metadata name="My custom checks"  priority="1600">
        <description>%MyCustomChecks.desc</description>

        <rule-metadata name="%MethodLimit.name" internal-name="MethodLimit" parent="TreeWalker">
            <alternative-name internal-name="net.sf.eclipsecs.sample.checks.MethodLimitCheck" />
            <description>%MethodLimit.desc</description>
            <property-metadata name="max" datatype="Integer" default-value="30">
                <description>The maximum number of allowed methods.</description>
            </property-metadata>
            <quickfix classname="net.sf.eclipsecs.sample.checks.MethodLimitQuickfix" />
        </rule-metadata>
        
        <rule-metadata name="Expressions Check" internal-name="ExpressionsCheck" parent="TreeWalker">
            <alternative-name internal-name="net.sf.eclipsecs.sample.checks.ExpressionsCheck" />
            <description>Counts the number of expressions</description>
        </rule-metadata>
        
        <rule-metadata name="Casts Check" internal-name="CastsCheck" parent="TreeWalker">
            <alternative-name internal-name="net.sf.eclipsecs.sample.checks.CastsCheck" />
            <description>Counts the number of casts</description>
        </rule-metadata>
        
        <rule-metadata name="Looping Statements Check" internal-name="LoopingStatementsCheck" parent="TreeWalker">
            <alternative-name internal-name="net.sf.eclipsecs.sample.checks.LoopingStatementsCheck" />
            <description>Counts the number of looping statements</description>
        </rule-metadata>
        
        <rule-metadata name="Variable Declarations Check" internal-name="VariableDeclarationsCheck" parent="TreeWalker">
            <alternative-name internal-name="net.sf.eclipsecs.sample.checks.VariableDeclarationsCheck" />
