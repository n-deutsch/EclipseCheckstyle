            <alternative-name internal-name="net.sf.eclipsecs.sample.checks.HalsteadEffortCheck" />
            <description>Calculates the Halstead effort</description>
        </rule-metadata>
        
        <rule-metadata name="Maintainability Index Check" internal-name="MaintainabilityIndexCheck" parent="TreeWalker">
            <alternative-name internal-name="net.sf.eclipsecs.sample.checks.MaintainabilityIndexCheck" />
            <description>Calculates the maintainability index</description>
