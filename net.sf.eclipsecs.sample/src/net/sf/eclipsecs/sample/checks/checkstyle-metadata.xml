        <rule-metadata name="Comment Lines Check" internal-name="CommentLinesCheck" parent="TreeWalker">
            <alternative-name internal-name="net.sf.eclipsecs.sample.checks.CommentLinesCheck" />
            <description>Counts the number of lines of comments</description>
        </rule-metadata>
        
        <rule-metadata name="Halstead Difficulty Check" internal-name="HalsteadDifficultyCheck" parent="TreeWalker">
            <alternative-name internal-name="net.sf.eclipsecs.sample.checks.HalsteadDifficultyCheck" />
            <description>Calculates the Halstead Difficulty</description>
