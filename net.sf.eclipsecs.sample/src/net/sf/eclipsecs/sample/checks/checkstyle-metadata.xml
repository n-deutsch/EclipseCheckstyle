        <rule-metadata name="Operator Count Check" internal-name="OperatorCountCheck" parent="TreeWalker">
            <alternative-name internal-name="net.sf.eclipsecs.sample.checks.OperatorCountCheck" />
            <description>Counts the number of operators</description>
        </rule-metadata>
        
        <rule-metadata name="Local Method References Check" internal-name="LocalMethodCheck" parent="TreeWalker">
            <alternative-name internal-name="net.sf.eclipsecs.sample.checks.LocalMethodCheck" />
            <description>Counts the number of local method references</description>
        </rule-metadata>
        
        <rule-metadata name="External Method References Check" internal-name="ExternalMethodCheck" parent="TreeWalker">
            <alternative-name internal-name="net.sf.eclipsecs.sample.checks.ExternalMethodCheck" />
